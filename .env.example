# Public variables (included in frontend bundle)
NEXT_PUBLIC_BACKEND_URL=https://api.biocraft-studio.example.com

# Private variables (server-side only)
GOOGLE_GENAI_API_KEY=your-google-api-key
SUPABASE_URL=your-supabase-url
SUPABASE_ANON_KEY=your-supabase-anon-key
SUPABASE_DB_PASSWORD=your-supabase-db-password
FRONTEND_URL=https://biocraft-studio.example.com

# Social login configurations
GITHUB_CLIENT_ID=your-github-client-id
GITHUB_CLIENT_SECRET=your-github-client-secret
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret

# Enabling
NEXT_PUBLIC_ENABLE_SOCIAL_LOGIN=false
NEXT_PUBLIC_ENABLE_RECIPE_SHARING=false